    "@adonisjs/redis": "^7.3.4",
    "@adonisjs/repl": "^3.1.0",
    "@adonisjs/session": "^6.4.0",
    "luxon": "^3.4.4",
    "pg": "^8.11.3",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.2.1",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1"
